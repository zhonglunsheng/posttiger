    "monaco-editor": "^0.34.0",
    "ninja-keys": "^1.2.2",
    "node-sass": "^9.0.0",
    "prettier": "^3.1.0",
    "sql-formatter": "^9.2.0",
    "vconsole": "^3.15.1",
    "vue": "^3.2.45",
